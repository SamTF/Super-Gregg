resource/name = "PlatformSpikes.gd"
script/source = "extends StaticBody2D

onready var anim = get_node(\"Sprite\").get_node(\"Anim\")
onready var player = utils.main_node.get_node(\"Player\")

func _on_Area2D_body_enter( body ):
	if body.is_in_group(\"Player\") and body.pos < get_global_pos().y:
		#body.touchedSpikes = true
		body.set_linear_velocity(Vector2(0,-100))
		player.get_node(\"Sprite\").set_self_opacity(0)
		anim.play(\"TrapStart\")
		#body._on_exit_screen()
		#body.set_angular_velocity(5) #makes the player spin
		var collision = body.get_node(\"CollisionShape2D\")
		collision.queue_free() #removes the collision shape from the player
		print(\"OUCH\")
		

func _on_exit_screen():
	queue_free()
"

